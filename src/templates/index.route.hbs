import { Route } from '@angular/router';
{{#each fileImports}}
import { {{this.component}} } from '{{this.file}}';
{{/each}}

export const ROUTES: Route[] = [
    {
        path: '{{path}}',
        {{#if component}}
        component: {{component}}{{#if children.length}},{{/if}}
        {{/if}}
        {{#if children.length}}
        children: [
            {{> childRoutes children}}
        ]
        {{/if}}
    }
];