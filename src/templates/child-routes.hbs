{{#each this}}
{
    path: '{{this.route}}',
    {{#if this.component}}
    component: {{this.component}}{{#or this.providers this.matchers this.children.length}},{{/or}}
    {{/if}}
    {{#if this.providers}}
    providers: [...{{this.providers}}]{{#or this.matchers this.children.length}},{{/or}}
    {{/if}}
    {{#if this.matchers}}
    canMatch: [...{{this.matchers}}]{{#if this.children.length}},{{/if}}
    {{/if}}
    {{#if this.children.length}}
    children: [
        {{> childRoutes this.children}}
    ]
    {{/if}}
},
{{/each}}
